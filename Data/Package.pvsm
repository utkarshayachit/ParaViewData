<ServerManagerConfiguration>
  <ProxyGroup name="sources">
   <SourceProxy name="testConeSource" class="vtkConeSource">
      <IntVectorProperty
         name="Resolution"
         command="SetResolution"
         number_of_elements="1"
         default_values="6" >
        <IntRangeDomain min="0" max="512" />
      </IntVectorProperty>

      <DoubleVectorProperty
         name="Radius"
         command="SetRadius"
         number_of_elements="1"
         default_values="0.5" >
      </DoubleVectorProperty>

      <DoubleVectorProperty
         name="Height"
         command="SetHeight"
         number_of_elements="1"
         default_values="1.0" >
        <DoubleRangeDomain min="0" />
      </DoubleVectorProperty>

   <!-- End Cone -->
   </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
